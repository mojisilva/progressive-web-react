{
     "compilerOptions": {
         "target": "ES6",
         "baseUrl": "src",
         "jsx": "react",
         "paths": {
             "components:/*" :[
                 "./components/*"
             ],
             "container:/*":[
                "./container/*"
            ],
            "commons:/*" :[
                "./commons/*"
            ],
            "assets:/*" :[
                "./assets/*"
            ]
        }
     },
     "exclude": [
        "node_modules",
        "build"
     ]
}